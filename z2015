// Z2015.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "conio.h"
#include<iostream>
using namespace std; 
int main() {
	int a;
	int b;
	int c;
	int d;
	int e;
	float f;
	int g;
	cout << "Ingrese el primer numero:" << endl;
	cin >> a;
	cout << "Ingrese el segundo numero:" << endl;
	cin >> b;
	c = a+b;
	d = a-b;
	e = a*b;
	f = a/b;
	g = a%b;
	cout << "La suma es: " << c << endl;
	cout << "La resta es: " << d << endl;
	cout << "La multiplicacion es: " << e << endl;
	cout << "La division es: " << f << endl;
	cout << "El residuo es: " << g << endl;
	getch ();
	return 0;
}


